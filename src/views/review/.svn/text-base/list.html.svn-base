<table class="list">
{% for review in reviews %}
	<tr>
		<td class="title"><a onclick="return popUp('/review/detail/{{review.key}}')" href="#">{{ review.title }}</a> 
		{% if review.user%}
		By {{review.user.username}}</td>
		{% else %}
		By Anonymous Coward
		{% endif %}
	</tr>
{% endfor %}
</table>